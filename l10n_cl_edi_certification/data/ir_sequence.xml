<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Secuencia para Proyectos de Certificación -->
        <record id="seq_certification_project" model="ir.sequence">
            <field name="name">Proyecto de Certificación</field>
            <field name="code">l10n_cl_edi.certification.project</field>
            <field name="prefix">CERT-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Secuencia para Sobres de Envío -->
        <record id="seq_certification_envelope" model="ir.sequence">
            <field name="name">Sobre de Envío DTE</field>
            <field name="code">l10n_cl_edi.certification.envelope</field>
            <field name="prefix">ENV-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
